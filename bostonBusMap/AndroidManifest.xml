<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="boston.Bus.Map"
      android:versionCode="39"
      android:versionName="2.12">
	<application android:label="@string/app_name" android:icon="@drawable/bus" android:theme="@android:style/Theme.NoTitleBar"
	
	>
		<activity android:label="@string/app_name" android:name=".main.Main">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".main.Preferences" android:label="@string/app_name">
		</activity> 
		<uses-library android:name="com.google.android.maps"></uses-library>
	<uses-library android:name="android.test.runner"></uses-library>
</application>


	<uses-sdk android:minSdkVersion="3"></uses-sdk>
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>

	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>

<instrumentation android:name="android.test.InstrumentationTestRunner" android:targetPackage="boston.Bus.Map"></instrumentation>
</manifest>